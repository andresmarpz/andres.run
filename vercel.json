{
    "cleanUrls": true,
    "headers": [
        {
            "source": "/fonts/inter.woff2",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        }
    ],
    "redirects": [
        {
            "source": "/bookmarks",
            "destination": "https://bookmarks.andres.run"
        },
        {
            "source": "/files",
            "destination": "https://files.andres.run"
        }
    ]
}
